public ActionResult NotFound()
{
ViewBag["ErrorCode"]="NFE0001";
ViewBag["Description"]="La mascota no se encuentra"+"en la base de datos";
return View();
}

@{
    
    ViewBag.Title = "NotFound";
}
<h1> la información que buscaba no pudo ser encontrada </h1>
<h2>Detalles</h2>
<ul>
    <li> <b> Código de error</b>@ViewBag["ErrorCode"]</li>
    <li><b>Descripción:</b>@ViewBag["Description"]</li>

</ul>
<h3>La máscota no esta registrada</h3>
<img src="~/Content/Uploads/flecha.jpg" />
